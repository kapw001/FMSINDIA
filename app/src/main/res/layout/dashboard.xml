<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/dash"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background"
        android:gravity="center"
        android:padding="10dp"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="230dp"
                android:scaleType="centerInside"
                android:src="@drawable/banner" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0.5dp"
                android:layout_marginRight="0.5dp"
                android:id="@+id/rasiticket"
                android:layout_weight="1"

                android:orientation="vertical"


                android:padding="5dp">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="100dp"

                    android:gravity="center"

                    android:background="@drawable/smallbak"


                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:src="@drawable/rasiticket" />

                    <com.fms.fmsindia.plugins.CustomTextView
                        android:id="@+id/rasieTicketText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Rasie Ticket"
                        android:textColor="#ffffff" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="0.5dp"
                android:layout_marginLeft="0.5dp"
                android:id="@+id/openticket"
                android:layout_weight="1"

                android:orientation="vertical"
                android:padding="5dp">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="100dp"

                    android:background="@drawable/smallbak"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:src="@drawable/openticket" />

                    <com.fms.fmsindia.plugins.CustomTextView
                        android:id="@+id/openTicketText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Ticket Status"
                        android:textColor="#ffffff" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="0.5dp"
                android:layout_marginRight="0.5dp"
                android:layout_marginTop="0.5dp"
                android:id="@+id/closedticket"
                android:layout_weight="1"

                android:orientation="vertical"
                android:padding="5dp">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="100dp"


                    android:gravity="center"
                    android:background="@drawable/smallbak"

                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                            android:src="@drawable/closedticket" />

                    <com.fms.fmsindia.plugins.CustomTextView
                        android:id="@+id/closedTicketText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Closed Tickets"
                        android:textColor="#ffffff" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0.5dp"
                android:layout_marginRight="0.5dp"
                android:id="@+id/profiles"
                android:layout_weight="1"

                android:orientation="vertical"


                android:padding="5dp">

                <LinearLayout
                    android:id="@+id/profiles"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"

                    android:gravity="center"

                    android:background="@drawable/smallbak"


                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:src="@drawable/profiles" />

                    <com.fms.fmsindia.plugins.CustomTextView
                        android:id="@+id/profileTicket"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Profile"
                        android:textColor="#ffffff" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="0.5dp"
                android:layout_marginLeft="0.5dp"
                android:id="@+id/feedbacks"
                android:layout_weight="1"

                android:orientation="vertical"
                android:padding="5dp">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="100dp"

                    android:background="@drawable/smallbak"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:src="@drawable/feedbacks" />

                    <com.fms.fmsindia.plugins.CustomTextView
                        android:id="@+id/feedbacksText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Feedback"
                        android:textColor="#ffffff" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="0.5dp"
                android:layout_marginRight="0.5dp"
                android:layout_marginTop="0.5dp"
                android:id="@+id/partnerus"
                android:layout_weight="1"

                android:orientation="vertical"
                android:padding="5dp">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="100dp"


                    android:gravity="center"
                    android:background="@drawable/smallbak"

                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:src="@drawable/partnerus" />

                    <com.fms.fmsindia.plugins.CustomTextView
                        android:id="@+id/partnerusText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Partner Us"
                        android:textColor="#ffffff" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

</LinearLayout>

    </LinearLayout>



